@model IEnumerable<SBD.Models.Transfuzja>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.PotrzebnaIlosc)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DataTransfuzji)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Badania)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Pacjent)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Pielegniarka)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.PotrzebnaIlosc)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DataTransfuzji)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Badania.Badaniaid)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Pacjent.Pacjentid)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Pielegniarka.Pielegniarkaid)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Transfuzjaid">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Transfuzjaid">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Transfuzjaid">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
